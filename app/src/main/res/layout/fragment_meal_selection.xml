<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data>
        <variable
            name="viewModelCategories"
            type="android.example.tableforone.mealCateorySelect.MealCategorySelectViewModel" />
    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/mealSelectLayout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".MealSelectionFragment">

        <androidx.recyclerview.widget.RecyclerView
            android:id="@+id/mealSelectRecyclerView"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>